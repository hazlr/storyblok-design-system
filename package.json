{
  "name": "storyblok-design-system",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib ./src/main.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint src/ tests/ .storybook/",
    "build-storybook": "build-storybook -s ./public",
    "storybook": "start-storybook -p 6006 -s ./public --ci"
  },
  "main": "dist/storyblok-design-system.umd.min.js",
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.6",
    "portal-vue": "^2.1.7",
    "throttle-debounce": "^2.3.0"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-essentials": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^6.0.28",
    "@storybook/vue": "^6.0.28",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-eslint": "~4.5.8",
    "@vue/cli-plugin-unit-jest": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "chromatic": "^5.4.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "eslint-plugin-vue": "^7.1.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.0.5",
    "style-loader": "^2.0.0",
    "style-resources-loader": "^1.3.2",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
